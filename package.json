    "homepage": "https://github.com/npmtest/node-npmtest-verb",
    "keywords": [
        "coverage",
        "test",
        "verb"
    ],
    "license": "MIT",
    "main": "lib.npmtest_verb.js",
    "name": "npmtest-verb",
    "nameAlias": "npmtest_verb",
